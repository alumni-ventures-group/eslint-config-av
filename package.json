{
  "name": "@alumni-ventures-group/eslint-config-av",
  "version": "0.1.4",
  "description": "Shared eslintrc files for AV projects",
  "main": "index.js",
  "scripts": {
    "semantic-release": "semantic-release"
  },
  "author": "",
  "license": "ISC",
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "dependencies": {
    "@babel/core": "^7.21.4",
    "@babel/eslint-parser": "^7.21.3",
    "@rushstack/eslint-patch": "^1.2.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "eslint-config-airbnb": "^19",
    "eslint-config-next": "^13.4.3",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-import": "^2",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6",
    "eslint-plugin-react": "^7",
    "semantic-release": "^21.0.1"
  },
  "peerDependencies": {
    "eslint": "^8"
  },
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/changelog",
    "@semantic-release/github",
    "@semantic-release/npm",
    "@semantic-release/git"
  ],
  "release": {
    "branches": [
      "main"
    ],
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      {
        "path": "@semantic-release/git",
        "assets": [
          "package.json",
          "package-lock.json",
          "CHANGELOG.md"
        ],
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alumni-ventures-group/eslint-config-av.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@alumni-ventures-group"
  }
}
